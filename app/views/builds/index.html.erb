<div class="row">
  <div class="col-md-3">
    <div class="panel panel-default">
      <div class="panel-heading">Repos</div>
      <ul class="list-group">
        <% 10.times do %>
          <li class="list-group-item">
            <%= link_to "#", class: "btn-setting quiet", "data-toggle" => "tooltip", "data-placement" => "top", title: "Setting" do %><i class="fa fa-cog"></i><% end %>
            <%= link_to "user/name", "#" %>
          </li>
        <% end %>
      </ul>
    </div>
  </div>
  <div class="col-md-9">
    <div class="panel panel-default">
      <div class="panel-heading">Builds</div>
      <ul class="list-group">
        <% 10.times.each do %>
        <li class="list-group-item">
          <div class="row build">
            <div class="col-sm-2">
              <p>
                <%= link_to build_path(1), class: "btn btn-success btn-sm btn-block" do %>
                  <i class="fa fa-check"></i> <small>SUCCESS</small>
                <% end %>
              </p>
            </div>
            <div class="col-sm-7">
              <ul>
                <li><%= link_to "company / my_ppoject / master", build_path(1) %></li>
                <li><strong>Scheduled build</strong></li>
                <li>Rubocop <i class="fa fa-arrow-right"></i> API Server</li>
              </ul>
            </div>
            <div class="col-sm-3">
              <ul>
                <li><i class="fa fa-calendar-o"></i> 7 hour ago</li>
                <li><i class="fa fa-clock-o"></i> 01:52</li>
                <li><i class="fa fa-git"></i> <%= link_to "88bb865", "#", class: "quiet" %></li>
              </ul>
            </div>
          </div>
        </li>
        <% end %>
      </ul>
    </div>
    <nav aria-label="Page navigation">
      <ul class="pagination pagination-sm">
        <li>
          <a href="#" aria-label="Previous">
            <span aria-hidden="true">&laquo;</span>
          </a>
        </li>
        <li><a href="#">1</a></li>
        <li><a href="#">2</a></li>
        <li><a href="#">3</a></li>
        <li><a href="#">4</a></li>
        <li><a href="#">5</a></li>
        <li>
          <a href="#" aria-label="Next">
            <span aria-hidden="true">&raquo;</span>
          </a>
        </li>
      </ul>
    </nav>
  </div>
</div>
